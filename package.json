    "@angular/forms": "^15.2.6",
    "@angular/platform-browser": "^15.2.6",
    "@angular/platform-browser-dynamic": "^15.2.6",
    "@angular/router": "^15.2.6",
    "@kingfar/ui-angular": "file:dist/ui-angular",
    "@swimlane/ngx-ui": "^44.6.0",
    "@types/md5": "^2.3.2",
    "ace-builds": "^1.21.1",
    "autoprefixer": "^10.4.14",
    "brace": "^0.11.1",
    "caniuse-lite": "^1.0.30001485",
    "cherry-markdown": "^0.8.19",
    "clean-css": "^5.3.2",
    "cpx": "^1.5.0",
