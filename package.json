{
  "name": "my-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4500",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build_lib": "ng build ui-angular --prod",
    "build:css": "node scripts/prep-global.styles.js && npm run copy-files",
    "copy-files": "cpx ./README.md ./dist/ui-angular/ && cpx LICENSE dist/ui-angular/",
    "package": "npm run build_lib && npm install @kingfar/ui-angular && npm run start",
    "ui-css": "ng build ui-angular && node node scripts/prep-global.styles.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.6",
    "@angular/cdk": "^15.2.6",
